{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "npx nodemon",
    "lint": "npx eslint ./src",
    "test": "jest",
    "format": "npx eslint ./src --fix",
    "typeorm": "typeorm-ts-node-esm",
    "migration:generate": "cross-var npm run typeorm migration:generate -- src/db/migrations/$npm_config_name -d src/db/db.connector.ts",
    "migration:create": "cross-var npm run typeorm migration:create -- ./src/db/migrations/$npm_config_name",
    "seed:create": "cross-var npm run typeorm migration:create -- ./src/db/consts/$npm_config_name",
    "migration:run": "npm run typeorm migration:run -- -d src/db/db.connector.ts",
    "migration:revert": "npm run typeorm migration:revert -- -d src/db/db.connector.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/minio": "^7.1.1",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.3.1",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "eslint": "^8.43.0",
    "jest": "^29.5.0",
    "rimraf": "^5.0.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-var": "^1.1.0",
    "csv": "^6.3.1",
    "dotenv": "^16.3.0",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.11.0",
    "ts-node": "^10.9.1",
    "typeorm": "^0.3.16"
  }
}
